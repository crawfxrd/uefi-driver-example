# EditorConfig
# https://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[Makefile]
indent_style = tab

[*.inf]
indent_size = 2

[*.md]
indent_size = 2

[*.{yaml,yml}]
indent_size = 2
